# BrowserStack Credentials
BROWSERSTACK_USERNAME=your_browserstack_username
BROWSERSTACK_ACCESS_KEY=your_browserstack_access_key

# App URLs in BrowserStack
ANDROID_APP_URL=bs://your_android_app_id
IOS_APP_URL=bs://your_ios_app_id

# Test Configuration
PLATFORM=android
DEVICE_NAME=Google Pixel 7
OS_VERSION=13.0

# iOS specific (when PLATFORM=ios)
# DEVICE_NAME=iPhone 14 Pro
# OS_VERSION=16

# BrowserStack Configuration
BROWSERSTACK_PROJECT_NAME=Mobile Testing Framework
BROWSERSTACK_BUILD_NAME=Build_1.0
BROWSERSTACK_DEBUG=true
BROWSERSTACK_NETWORK_LOGS=true
